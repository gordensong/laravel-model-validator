{
    "name": "gordensong/laravel-model-validator",
    "description": "Generate Laravel model or table validator from your existing models or tables",
    "keywords": ["Laravel", "Model", "Validator"],
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "gordensong",
            "email": "94581976@qq.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "5.5.*|5.6.*|~5.8|^6.0|^7.0",
        "illuminate/console": "5.5.*|5.6.*|~5.8|^6.0|^7.0",
        "illuminate/filesystem": "5.5.*|5.6.*|~5.8|^6.0|^7.0",
        "illuminate/database": "5.5.*|5.6.*|~5.8|^6.0|^7.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^5.5",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "GordenSong\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "LaravelModelValidatorServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": ["tests"]
    }
}
